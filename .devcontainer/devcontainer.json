{
  "name": "NetWall Environment",
  "image": "mcr.microsoft.com/devcontainers/python:3.10",
  "features": {
    "ghcr.io/devcontainers/features/java:1": {
      "version": "17"
    }
  },
  "postCreateCommand": "pip install -r requirements.txt && curl -O https://www.antlr.org/download/antlr-4.13.1-complete.jar",
  "customizations": {
    "vscode": {
      "extensions": ["ms-python.python", "mike-lischke.vscode-antlr4"]
    }
  }
}